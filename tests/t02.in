// operation/printf test

int main() {
    printf("%d * (%d + %d) = %d\n", 5, 6, 7, 5 * (6 + 7))
    printf("(%d * %d) + %d = %d\n", 5, 6, 7, (5 * 6) + 7)
    printf("%d * %d + %d = %d\n", 5, 6, 7, 5 * 6 + 7)
    printf("%s %s\n", "string", "specifiers")
    printf("%c%c", '0' + '?', '4' + '7')
    // '0' = 48, '?' = 63, '0' + '?' = 111 = 'o'
    // '4' = 52, '7' = 55, '4' + '7' = 107 = 'k'
    return 0
}