// Variable assignment test

int main() {
    int x = 5
    bool b = x == 4
    if (b == true) {
        string string_of_bool = "true"
        printf("x == %d: %s", 4, string_of_bool)
    }
    else {
        string string_of_bool = "false"
        printf("x == %d: %s", 4, string_of_bool)
    }
    return 0
}